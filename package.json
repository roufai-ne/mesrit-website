    "jsdom": "^23.2.0",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.468.0",
    "mongoose": "^8.9.2",
    "multer": "^2.0.0",
    "next": "^15.1.1-canary.23",
    "node-cache": "^5.1.2",
