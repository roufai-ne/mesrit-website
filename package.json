    "@types/react-dom": "^19",
    "@types/tailwindcss": "^3.0.11",
    "autoprefixer": "^10.4.20",
    "cross-env": "^10.1.0",
    "eslint": "^9.21.0",
    "eslint-config-next": "15.0.4",
    "jest": "^30.1.3",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.20.6",
    "typescript": "^5"
