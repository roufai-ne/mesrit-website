{
  "name": "mesrit-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "dev:network": "next dev -H 0.0.0.0 -p 3000",
    "dev:local": "next dev",
    "build": "next build",
    "build:prod": "NODE_ENV=production next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start -p 3000",
    "lint": "next lint",
    "test": "jest",
    "test:security": "jest tests/security.test.js",
    "test:watch": "jest --watch",
    "create-admin": "node -r esm scripts/createAdmin.js",
    "generate-secrets": "node scripts/generateSecrets.js",
    "setup": "npm run generate-secrets && npm run create-admin",
    "export": "next export",
    "analyze": "ANALYZE=true npm run build",
    "migrate:v2": "node scripts/migrateAnalytics.js",
    "validate:v2": "node scripts/validateMigrationV2.js",
    "setup:v2": "npm run migrate:v2 && npm run validate:v2",
    "maintenance:v2": "node scripts/autoMaintenanceV2.js",
    "optimize:v2": "node scripts/performanceOptimizerV2.js"
  },
  "dependencies": {
